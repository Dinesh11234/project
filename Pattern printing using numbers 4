package com.dinesh;
import java.util.Scanner;

public class Main {

    public static void main( String[] args) {
        int i,j,n,p=1;
        Scanner input = new Scanner(System.in);
        System.out.print("enter a number:");
        n= input.nextInt();
                for(i=1;i<=n;i++){
                    for(j=1;j<=i;j++){
                        System.out.print(p+++" ");
                   }
                    System.out.println();

    }


}
}
